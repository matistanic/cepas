<p id="notice"><%= notice %></p>

<h1>Oenologist Magazines</h1>

<table>
  <thead>
    <tr>
      <th>Oenologist</th>
      <th>Magazine</th>
      <th>Position</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @oenologist_magazines.each do |oenologist_magazine| %>
      <tr>
        <td><%= oenologist_magazine.oenologist %></td>
        <td><%= oenologist_magazine.magazine %></td>
        <td><%= oenologist_magazine.position %></td>
        <td><%= link_to 'Show', oenologist_magazine %></td>
        <td><%= link_to 'Edit', edit_oenologist_magazine_path(oenologist_magazine) %></td>
        <td><%= link_to 'Destroy', oenologist_magazine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Oenologist Magazine', new_oenologist_magazine_path %>
