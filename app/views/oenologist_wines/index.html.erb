<p id="notice"><%= notice %></p>

<h1>Oenologist Wines</h1>

<table>
  <thead>
    <tr>
      <th>Oenologist</th>
      <th>Wine</th>
      <th>Grade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @oenologist_wines.each do |oenologist_wine| %>
      <tr>
        <td><%= oenologist_wine.oenologist %></td>
        <td><%= oenologist_wine.wine %></td>
        <td><%= oenologist_wine.grade %></td>
        <td><%= link_to 'Show', oenologist_wine %></td>
        <td><%= link_to 'Edit', edit_oenologist_wine_path(oenologist_wine) %></td>
        <td><%= link_to 'Destroy', oenologist_wine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Oenologist Wine', new_oenologist_wine_path %>
